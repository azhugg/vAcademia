    CONTROL         "",IDC_STATIC,"Static",SS_ETCHEDHORZ,57,11,342,1
    LTEXT           "Step",IDC_STEP,57,0,342,8
    LTEXT           "Description",IDC_DESCRIPTION,57,14,342,36
    LISTBOX         IDC_MATERIAL,58,61,342,199,LBS_SORT | 
