<?xml version="1.0"?>
<window>
  <!-- Define all bitmaps used in skin -->
  <bitmaps>
    <bitmap name="timeline" src="timeline.bmp"/>
  </bitmaps>
  <!-- Define all form`s elements to be skinned -->
  <skin>
    <level valign="Top">
      <element align="Left" srcRect="0,97,108,60"    bitmap="timeline"  />
      <element align="Right" srcRect="208,97,12,60"    bitmap="timeline"  />
      <element align="Justify" srcRect="108,97,100,60" bitmapTile="true"   bitmap="timeline"  />
    </level>
  </skin>
  <controls>
      <control name="btnPlay"   srcRect="0,0,220,55" bitmap="timeline"/>
      <control name="btnRewind"  srcRect="0,55,168,42" bitmap="timeline"/>
  </controls>
  <properties>
    <property name="btnPlay.BorderStyle" value="None"/>
	<property name="btnRewind.BorderStyle" value="None"/>
  </properties>
